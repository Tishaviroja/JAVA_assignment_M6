package com.oops;

import java.util.Scanner;

public class prime_num 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println(" Enter Any No:");
		int num=sc.nextInt();
		for(int i=1; i<=500; i++)
		{
			int prime=0;
			for(int j=2; j<=i/2; j++)
			{
				if(i%j == 0)
				{
					prime++;
					
				}
				
			}
			if (prime < 0)
			{
	            System.out.println(i + " is a prime number");
	       } 
	   }

	}
}

