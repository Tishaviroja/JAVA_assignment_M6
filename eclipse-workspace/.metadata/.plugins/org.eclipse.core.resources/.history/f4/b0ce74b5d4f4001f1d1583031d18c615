package com.oops;

import java.util.Scanner;

public class Fibonaci 
{
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter No :");
		int num=sc.nextInt();
		
		for(int i=0; i<=num; i++)
		{
			num=num+i;
		}
		System.out.println("Fibonacci series....... "+n);
	}

}
